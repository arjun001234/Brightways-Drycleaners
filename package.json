{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "local": "dotenv -e .env.dev npm run dev",
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "remix build && run-p \"dev:*\"",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "dev:remix": "remix watch",
    "generate:css": "npx tailwindcss -o ./app/tailwind.css",
    "dev:node": "dotenv -e .env.dev nodemon ./server.js --watch ./server.js",
    "start": "dotenv -e .env.prod node ./server.js",
    "cypress:open": "dotenv -e .env.test cypress open",
    "update-types": "npx openapi-typescript \"${SUPABASE_URL}/rest/v1/?apikey=${SUPABASE_ANON_KEY}\" --version=2 --output types/database/index.ts"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.154.0",
    "@react-google-maps/api": "^2.12.1",
    "@remix-run/express": "^1.6.0",
    "@remix-run/node": "^1.6.0",
    "@remix-run/react": "^1.6.0",
    "@supabase/supabase-js": "^1.35.3",
    "axios": "^0.27.2",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "express": "^4.18.1",
    "framer-motion": "^6.3.11",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.8",
    "pino": "^8.4.2",
    "react": "^17.0.2",
    "react-countup": "^6.2.0",
    "react-dom": "^17.0.2",
    "react-google-recaptcha": "^2.1.0",
    "react-icons": "^4.4.0",
    "react-intersection-observer": "^9.4.0",
    "react-toastify": "^9.0.4",
    "tailwindcss-animation-delay": "^1.0.7",
    "tiny-invariant": "^1.2.0",
    "twilio": "^3.77.3"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.127.0",
    "@remix-run/dev": "^1.6.0",
    "@remix-run/eslint-config": "^1.6.0",
    "@types/nodemailer": "^6.4.4",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@types/react-google-recaptcha": "^2.1.5",
    "cypress": "^10.1.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.15.0",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "openapi-typescript": "^5.4.0",
    "tailwindcss": "^3.1.3",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=16"
  }
}
