{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "dotenv -e .env.prod remix build",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "dev": "remix build && run-p dev:*",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "dev:node": "dotenv -e .env.dev nodemon ./server.js --watch ./server.js",
    "dev:remix": "dotenv -e .env.dev remix watch",
    "start": "dotenv -e .env.prod node ./server.js",
    "cypress:open": "dotenv -e .env.test cypress open",
    "migrate:postgres:dev": "dotenv -e .env.dev -- npx prisma migrate dev --schema ./app/prisma/schema.prisma",
    "migrate:postgres:prod": "dotenv -e .env.prod -- npx prisma migrate deploy --schema ./app/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^3.15.2",
    "@remix-run/express": "^1.6.0",
    "@remix-run/node": "^1.6.0",
    "@remix-run/react": "^1.6.0",
    "@supabase/supabase-js": "^1.35.3",
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "express": "^4.18.1",
    "framer-motion": "^6.3.11",
    "morgan": "^1.10.0",
    "react": "^17.0.2",
    "react-countup": "^6.2.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "react-toastify": "^9.0.4",
    "tiny-invariant": "^1.2.0",
    "twilio": "^3.77.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.0",
    "@remix-run/eslint-config": "^1.6.0",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "cypress": "^10.1.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.15.0",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "prisma": "^3.15.2",
    "tailwindcss": "^3.1.3",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  }
}
